{
  "version": 2,
  "builds": [
    {
      "src": "Backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/static/index.html" },
    { "src": "/projects", "dest": "/static/projects.html" },
    { "src": "/api/(.*)", "dest": "Backend/server.js" }
  ]
}
